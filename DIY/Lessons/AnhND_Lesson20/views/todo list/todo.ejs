<div class="forecast-container">
	<div class = "todo-list-container">
	   <div class="todo-list-header">
		 <h2>TODO List</h2>
		 <input type="text" placeholder="Title...">
		 <span class="addBtn">Add</span>
	   </div>
	   <ul class = "todo-list">
		<% for(const todo of todos) { %>
			<li class="<% if (todo.checked) { %> checked <% } %>">
				<%= todo.title %>
			</li>
		<% } %>
	   </ul>
	</div>
 </div>
